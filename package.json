{
  "name": "bmart-1",
  "version": "0.5.0",
  "proxy": "http://localhost:13100",
  "dependencies": {
    "@prisma/client": "^2.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.45",
    "@types/react-dom": "^16.9.8",
    "app-root-path": "^3.0.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "smoothscroll-polyfill": "^0.4.4",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "pm2:start": "if pm2 show bmart ; then npm run pm2:stop && npm run pm2:delete && npm run pm2:start ; else pm2 start src/server/build/server/main.js --name \"bmart\" ; fi",
    "pm2:restart": "if pm2 show bmart ; then pm2 restart bmart ; else echo 'No PM2 instance for bmart' ; fi",
    "pm2:stop": "if pm2 show bmart ; then pm2 stop bmart ; else echo 'No PM2 instance for bmart' ; fi",
    "pm2:delete": "if pm2 show bmart ; then pm2 delete bmart ; else echo 'No PM2 instance for bmart' ; fi",
    "pm2:reboot": "npm install && npm run build && npm run pm2:stop && npm run pm2:delete && npm run pm2:start",
    "start": "node src/server/build/server/main.js",
    "dev": "npm run prisma:generate && npm-run-all --parallel dev:client dev:server",
    "build": "npm run build:client && npm run build:server",
    "dev:client": "cross-env PORT=4000 craco start",
    "build:client": "craco build",
    "test:client": "craco test",
    "eject:client": "craco eject",
    "dev:server": "cross-env NODE_ENV=development TS_NODE_PROJECT=src/server/tsconfig.json tsnd --no-notify --respawn --transpile-only -r tsconfig-paths/register src/server/main.ts",
    "build:server": "ttsc --project src/server/tsconfig.json",
    "prisma:introspect": "prisma introspect --experimental-reintrospection",
    "prisma:generate": "prisma generate",
    "prisma:all": "npm run prisma:introspect && npm run prisma:generate",
    "prisma:studio": "prisma studio --experimental",
    "typecheck": "tsc --project src/server/tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "ts-node:server": "env TS_NODE_PROJECT=src/server/tsconfig.json npx ts-node -r tsconfig-paths/register",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@craco/craco": "^5.6.4",
    "@prisma/cli": "^2.5.1",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-essentials": "^6.0.16",
    "@storybook/addon-links": "^6.0.16",
    "@storybook/node-logger": "^6.0.16",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.16",
    "@types/app-root-path": "^1.2.4",
    "@types/classnames": "^2.2.10",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/passport": "^1.0.4",
    "@types/passport-github": "^1.1.5",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "csv-parser": "^2.3.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "http-proxy-middleware": "^1.0.5",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-is": "^16.13.1",
    "sass": "^1.26.10",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node-dev": "^1.0.0-pre.60",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10"
  }
}
